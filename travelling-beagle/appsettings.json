{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ExternalServices": {
    "Countries": {
      "Url": "https://restcountries.eu/rest/v2"
    },
    "TravelAdvisories": {
      "Url": "http://www.reisewarnung.net/api"
    },
    "Images": {
      "Url": "https://pixabay.com/api",
      "ApiKeyEnvironmentVariable": "TB_IMAGES_API_KEY"
    },
    "Google": {
      "TimezoneUrl": "https://maps.googleapis.com/maps/api/timezone/json",
      "GeoCodeUrl": "https://maps.googleapis.com/maps/api/geocode/json",
      "ApiKeyEnvironmentVariable": "TB_GOOGLE_API_KEY"
    },
    "Weather": {
      "Url": "http://api.openweathermap.org/data/2.5/weather",
      "ApiKeyEnvironmentVariable": "TB_WEATHER_API_KEY"
    },
    "Reddit": {
      "AuthorizeUrl": "https://www.reddit.com/api/v1/access_token?grant_type=client_credentials",
      "Url": "https://oauth.reddit.com",
      "ClientIdEnvironmentVariable": "TB_REDDIT_ID",
      "ClientSecretEnvironmentVariable": "TB_REDDIT_SECRET"
    }
  }
}
